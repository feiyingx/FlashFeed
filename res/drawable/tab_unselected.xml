<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:top="30dip">
		<shape android:shape="rectangle">
			<solid android:color="#f2f2ea" />
		</shape>
	</item>
	<item android:top="30dip" android:bottom="1px">
		<shape android:shape="rectangle">
			<solid android:color="#f2f2ea" />
		</shape>
	</item>
	<item android:left="0dip" android:right="1dip"> 
	
		<shape android:shape="rectangle">
			<corners android:bottomLeftRadius="0dip"
				android:bottomRightRadius="0dip" 
				android:topLeftRadius="0dip"
				android:topRightRadius="0dip"
				/>
				
			<solid android:color="#4d4d4d" />
			<!--  <stroke android:width="1px" android:color="#4d4d4d" /> -->
		</shape>
	</item>
</layer-list>